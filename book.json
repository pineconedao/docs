
{
    "title": "PineconeDAO",
    "author": "Gavin", 
    "description": "Pine cone", 
    "language": "zh", 
    "gitbook": "3.2.3",
    "links": {
        "sidebar": {
            "中文": "/zh/",
            "English": "/en/"
        }
    },
    "plugins": [
        "-highlight",
        "-lunr",
        "-search",
        "search-pro",
        "-sharing",
        "-sharing-plus",
        "fontsettings",
        "livereload",
        "expandable-chapters-small",
        "chapter-fold",
        "splitter",
        "hide-element",
        "back-to-top-button",
        "favicon",
        "insert-logo",
        "pageview-count",
        "code",
        "prism",
        "lightbox",
        "-github",
        "-github-buttons",
        "-donate",
        "anchor-navigation-ex",
        "meta",
        "-mygitalk",
        "change_girls",
        "simple-mind-map",
        "image-captions",
        "-todo",
        "edit-link-plus",
        "-sitemap-general",
        "-rss",
        "theme-comscore",
        "page-copyright"
    ],
    "pluginsConfig": {
        "sharing": {
            "douban": true,
            "facebook": false,
            "google": true,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": true,
            "qzone": true,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": [
                "douban",
                "facebook",
                "google",
                "hatenaBookmark",
                "instapaper",
                "line",
                "linkedin",
                "messenger",
                "pocket",
                "qq",
                "qzone",
                "stumbleupon",
                "twitter",
                "viber",
                "vk",
                "weibo",
                "whatsapp"
            ]
        },
        "hide-element": {
            "elements": [
                "a.gitbook-link[href='https://www.gitbook.com']"
            ]
        },
        "favicon": {
            "shortcut": "./resources/images/favicon.ico",
            "bookmark": "./resources/images/favicon.ico",
            "appleTouch": "./resources/images/favicon.png",
            "appleTouchMore": {
                "120x120": "./resources/images/apple-touch-icon-120x120.png",
                "180x180": "./resources/images/apple-touch-icon-180x180.png"
            }
        },
        "insert-logo": {
            "url": "/resources/images/favicon.png",
            "style": "background: none; max-height: 30px; min-height: 30px"
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-solarizedlight.css"
            ],
            "lang": {
                "shell": "bash"
            }
        },
        "lightbox": {
            "sameUuid": true 
        },
        "github": {
            "url": "https://github.com/pineconedao/docs"
        },
        "github-buttons": { 
            "buttons": [
                {
                    "user": "pudongping",
                    "repo": "glory",
                    "type": "star",
                    "count": true,
                    "size": "small"
                }
            ]
        },
        "anchor-navigation-ex": {
            "showLevel": true, 
            "showGoTop": false 
        },
        "meta": {
            "data": [
                {
                    "name": "referrer",
                    "content": "never"
                }
            ]
        },
        "mygitalk": {
            "clientID": "",  
            "clientSecret": "",  
            "repo": "",  
            "owner": "", 
            "admin": [ 
                "admin-1",
                "admin-2"
            ],
            "distractionFreeMode": false 
        },
        "simple-mind-map": {
            "type": "markdown",
            "preset": "colorful",
            "linkShape": "diagonal",
            "autoFit": true
        },
        "image-captions": {
            "caption": "PineconeDAO - _PAGE_LEVEL_._PAGE_IMAGE_NUMBER_ - _CAPTION_"
        },
        "edit-link-plus": {
            "base": {
                "edit-link-name-1": "edit-link-1",
                "edit-link-name-2": "edit-link-2"
            },
            "defaultBase": "", 
            "label": "edit"
        },
        "page-copyright": {
            "description": "modified at:",
            "signature": "Pinecone DAO",
            "wisdom": "Pine Cone DAO Play and Earn Metaverse Games",
            "format": "YYYY-MM-dd hh:mm:ss",
            "copyright": "Copyright &#169; PineconeDAO",
            "timeColor": "#666",
            "copyrightColor": "#666",
            "utcOffset": "8",
            "style": "normal",
            "noPowered": true,
            "baseUri": "https://pineconedao.com"
        }
    }
}